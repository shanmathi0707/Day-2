import java.util.*;
public class main
{
  public static void main(String args[])
  {
     int num1,num2,sum;
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter The Two Numbers:");
     num1=sc.nextInt();
     num2=sc.nextInt();
     sum = num1 + num2;
     System.out.println(sum);
    if(sum%2 == 0)
           {
              System.out.println("Sum is even");              
           }
        else
         {
            System.out.println("Sum is odd");
         }
 }

}
