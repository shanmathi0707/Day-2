import java.util.*;
class Checker
{
  public static void main(String args[])
  {
     int num1;
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter the Number:");
     num1=sc.nextInt();
        if(num1%2 == 0)
           {
              System.out.println("Number is even");              
           }
        else
         {
            System.out.println("Number is odd");
         }

 }

}
