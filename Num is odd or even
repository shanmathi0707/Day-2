import java.util.*;
public class main
{
  public static void main(String args[])
  {
     int num1;
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter the Number:");
     num1=sc.nextInt();
    if(num1%2 == 0)
           {
              System.out.println("num is even");              
           }
        else
         {
            System.out.println("num is odd");
         }
 }

}
